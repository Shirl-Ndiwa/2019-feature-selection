  file = {/Users/pjs/Zotero/storage/B6H4KC5K/VOGELMANN et al. - 1993 - Red edge spectral measurements from sugar maple le.pdf;/Users/pjs/Zotero/storage/HGIDD8WY/01431169308953986.html},
