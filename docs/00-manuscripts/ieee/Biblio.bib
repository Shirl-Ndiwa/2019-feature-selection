  journal = {Remote Sensing},
  keywords = {drought monitoring,Kruger National Park,Landsat-8,NDVI,savanna ecosystems,Sentinel-1,Sentinel-2,surface moisture,time series analysis},
  language = {english},
  number = {9}
}

